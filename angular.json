{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "ngrx-crud-poc",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "ngrx-crud-poc": {
      "projectType": "application",
      "root": "apps/ngrx-crud-poc",
      "sourceRoot": "apps/ngrx-crud-poc/src",
      "prefix": "ngrx-crud-poc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ngrx-crud-poc",
            "index": "apps/ngrx-crud-poc/src/index.html",
            "main": "apps/ngrx-crud-poc/src/main.ts",
            "polyfills": "apps/ngrx-crud-poc/src/polyfills.ts",
            "tsConfig": "apps/ngrx-crud-poc/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ngrx-crud-poc/src/favicon.ico",
              "apps/ngrx-crud-poc/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/ngrx-crud-poc/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "scripts": ["node_modules/bootstrap/dist/js/bootstrap.min.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngrx-crud-poc/src/environments/environment.ts",
                  "with": "apps/ngrx-crud-poc/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngrx-crud-poc:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngrx-crud-poc:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrx-crud-poc:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ngrx-crud-poc/src/**/*.ts",
              "apps/ngrx-crud-poc/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ngrx-crud-poc"],
          "options": {
            "jestConfig": "apps/ngrx-crud-poc/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngrx-crud-poc-e2e": {
      "root": "apps/ngrx-crud-poc-e2e",
      "sourceRoot": "apps/ngrx-crud-poc-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngrx-crud-poc-e2e/cypress.json",
            "tsConfig": "apps/ngrx-crud-poc-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngrx-crud-poc:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngrx-crud-poc:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ngrx-crud-poc-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "backend": {
      "root": "apps/backend",
      "sourceRoot": "apps/backend/src",
      "projectType": "application",
      "prefix": "backend",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/backend",
            "main": "apps/backend/src/main.ts",
            "tsConfig": "apps/backend/tsconfig.app.json",
            "assets": ["apps/backend/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/backend/src/environments/environment.ts",
                  "with": "apps/backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "backend:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/backend/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/backend"],
          "options": {
            "jestConfig": "apps/backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-data": {
      "projectType": "library",
      "root": "libs/core-data",
      "sourceRoot": "libs/core-data/src",
      "prefix": "ngrx-crud-poc",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core-data/src/**/*.ts",
              "libs/core-data/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-data"],
          "options": {
            "jestConfig": "libs/core-data/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ngrx-crud-util": {
      "projectType": "library",
      "root": "libs/ngrx-crud-util",
      "sourceRoot": "libs/ngrx-crud-util/src",
      "prefix": "ngrx-crud-poc",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ngrx-crud-util/src/**/*.ts",
              "libs/ngrx-crud-util/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ngrx-crud-util"],
          "options": {
            "jestConfig": "libs/ngrx-crud-util/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "ngrx-crud-poc",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/src/**/*.ts", "libs/ui/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "books": {
      "projectType": "library",
      "root": "libs/books",
      "sourceRoot": "libs/books/src",
      "prefix": "ngrx-crud-poc",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/books/src/**/*.ts",
              "libs/books/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/books"],
          "options": {
            "jestConfig": "libs/books/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
